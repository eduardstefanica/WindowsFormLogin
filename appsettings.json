{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "D:\\LOGS\\WindowsFormsLogin\\Log_WindowsFormsLogin.txt" }
      }
    ]
  },
  "AllowedHosts": "*"
}